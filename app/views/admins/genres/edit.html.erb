<h2>ジャンル編集</h2>
<%= form_with model:[:admins, @genre] , local:true do |f| %>
<div class="form-group">
  <label for="inputName">ジャンル名</label>
  <div>
    <%= f.text_field :name, autofocus: true, id:"inputName" %>
  </div>
  <div class="form-group">
    <%= f.submit "変更を保存" %><!--class:"btn btn-success" %>-->
  </div>
<% end %>
</div>